    "styled-components": "^2.1.2",
    "toastr": "^2.1.2",
    "truefit-react-utils": "0.0.10",
    "universal-cookie": "^2.0.8",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
