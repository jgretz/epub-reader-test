    "babel-polyfill": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "numeral": "^2.0.4",
    "object-assign": "^4.1.1",
    "pluralize": "^7.0.0",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "^15.6.0",
    "react-dom": "^16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-loading-overlay": "^0.2.5",
    "react-number-format": "^2.0.4",
    "react-reader": "^0.16.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "reactcss": "^1.2.2",
    "redux": "^3.7.2",
    "redux-async-await": "^1.0.1",
